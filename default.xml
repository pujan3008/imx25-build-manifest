           path="sources/meta-freescale"
           revision="master"/>

  <project name="meta-freescale-distro"
           remote="freescale"
           path="sources/meta-freescale-distro"
           revision="master"/>

  <project name="meta-openembedded"
           remote="openembedded"
           path="sources/meta-openembedded"
           revision="master"/>

  <project name="i.MX25pdk-Custom-Layer-.git"
           remote="pujan"
           path="sources/meta-ei-imx25"
           revision="master"/>

